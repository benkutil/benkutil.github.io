  "bugs": {
    "url": "https://github.com/benkutil/benkutil.github.io/issues"
  },
  "homepage": "https://github.com/benkutil/benkutil.github.io#readme",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0-beta.3",
    "@11ty/eleventy-img": "^3.0.0",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "@eslint/js": "^9.39.2",
    "autoprefixer": "^10.4.24",
    "cssnano": "^7.1.2",
    "eleventy-plugin-webmentions": "^2.0.0",
    "eslint": "^9.39.2",
    "html-minifier-terser": "^7.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "luxon": "^3.5.0",
    "markdown-it-anchor": "^9.2.0",
    "markdown-it-attrs": "^4.3.1",
    "outdent": "^0.8.0",
    "pa11y-ci": "^4.0.1",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "prettier": "^3.8.1",
    "serve": "^14.2.5",
    "stylelint": "^17.1.0",
    "stylelint-config-standard": "^40.0.0",
    "wait-on": "^9.0.3"
  },
  "dependencies": {
    "@11ty/eleventy-upgrade-help": "^2.0.5",
